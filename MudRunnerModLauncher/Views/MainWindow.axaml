<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:MudRunnerModLauncher.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:MudRunnerModLauncher.Views"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="450"
        x:Class="MudRunnerModLauncher.Views.MainWindow"
        Icon="/Assets/logo.ico"
		xmlns:lang="clr-namespace:MudRunnerModLauncher.Lang"
        Title="{x:Static lang:Resource.ModRunnerModLauncherTitle}"
		MinHeight="310"
		MinWidth="400"
		Height="500"
		Width="600"
		WindowStartupLocation="CenterScreen">
	<views:LauncherView />
</Window>
